name: dockercompose12348751629212740242
services:
  mongodb:
    container_name: mongodb
    environment:
      MONGO_INITDB_DATABASE: salles
    image: mongo:6
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27018"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Mongo\mongo2\testMongo\testmongo\dbdata
        target: /data/db
        bind:
          create_host_path: true
      - type: bind
        source: C:\Mongo\mongo2\testMongo\testMongo\init-scripts
        target: /docker-entrypoint-initdb.d
        bind:
          create_host_path: true
  testmongo:
    build:
      context: C:\Mongo\mongo2\testMongo
      dockerfile: testMongo/Dockerfile
    depends_on:
      mongodb:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: testmongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\59011-82-08\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\59011-82-08\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose12348751629212740242_default